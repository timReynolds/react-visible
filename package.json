{
  "name": "react-visible",
  "version": "1.0.0",
  "description": "React componenet which controls the visibility of it's children",
  "main": "lib/Visible.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "build": "babel src --out-dir lib --ignore __tests__",
    "build:umd": "webpack src/Visible.js lib/Visible.js && NODE_ENV=production webpack src/Visible.js lib/Visible.min.js",
    "lint": "eslint src examples",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timReynolds/react-visible.git"
  },
  "keywords": [
    "React",
    "visible"
  ],
  "author": "timReynolds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timReynolds/react-visible/issues"
  },
  "homepage": "https://github.com/timReynolds/react-visible",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-jest": "^9.0.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "coveralls": "^2.11.8",
    "enzyme": "^2.0.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "jest-cli": "^0.8.2",
    "react": "^0.14.6",
    "react-addons-test-utils": "^0.14.6",
    "react-dom": "^0.14.6",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "react": "^0.14.6"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest/preprocessor.js",
    "testRunner": "<rootDir>/node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  }
}
