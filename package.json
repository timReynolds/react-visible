{
  "name": "react-visible",
  "version": "2.0.0",
  "description": "React component which controls the visibility of it's children",
  "main": "lib/Visible.js",
  "scripts": {
    "clean": "rimraf lib dist coverage",
    "build": "babel src --out-dir lib --ignore __tests__",
    "build:umd": "webpack src/Visible.js lib/Visible.js && NODE_ENV=production webpack src/Visible.js lib/Visible.min.js",
    "lint": "eslint src example",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timReynolds/react-visible.git"
  },
  "keywords": [
    "React",
    "visible"
  ],
  "author": "timReynolds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timReynolds/react-visible/issues"
  },
  "homepage": "https://github.com/timReynolds/react-visible",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "jest-cli": "^23.6.0",
    "react": "^16.7.0",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^16.7.0",
    "rimraf": "^2.5.0",
    "webpack": "^4.28.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^16.7.0"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  }
}
