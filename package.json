{
  "name": "react-visible",
  "version": "3.0.1",
  "description": "React component which controls the visibility of it's children",
  "main": "dist/Visible.js",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "parcel build src/Visible.js",
    "lint": "eslint src example/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepublish": "npm run lint && npm run test && npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/timReynolds/react-visible.git"
  },
  "keywords": [
    "React",
    "visible"
  ],
  "author": "timReynolds",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/timReynolds/react-visible/issues"
  },
  "homepage": "https://github.com/timReynolds/react-visible",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^6.7.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.0",
    "jest-cli": "^26.6.2",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3",
    "react": "^16.7.0",
    "react-addons-test-utils": "^15.0.1",
    "react-dom": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "rimraf": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "prop-types": "^15.6.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ]
  }
}
